{% extends 'base.html.twig' %}

{% block title %}Hello TestController!{% endblock %}

{% block body %}
    {% include '_partials/_menu.html.twig' %}
    <div class="container">
        <div class="row">
            {% for result in results %}
            <div class="row">
                <h1>
                    {{ result.import.annee }} - {{ result.import.fichier }}
                </h1>
                <div>
                    {% for data in result.datas %}
{#                        {{ dump(result) }}#}
                        <h6><a href="{{ path('app_tools_ajout_manuel', {dataId: data.id, importId: result.import.id}) }}">{{ data.id }}</a>
                            • {% if data.release_date != "" %}{{ data.release_date | date('Y') }}{% endif %} •
                            {{ data.title }}
                            [{{ data.original_title }}]
                        </h6>

                    {% endfor %}
                </div>

                <hr>
                {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}
